<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@*android:id/background">
        <shape>
            <gradient android:startColor="#ffebebeb" android:endColor="#ffebebeb" android:angle="90.0" android:centerY="0.75" />
        </shape>
    </item>
    <item android:id="@*android:id/progress">
        <clip>
            <shape>
                <gradient android:startColor="#ffffc000" android:endColor="#ffffc000" android:angle="90.0" />
            </shape>
        </clip>
    </item>
</layer-list>